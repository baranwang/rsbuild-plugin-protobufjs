{
  "name": "rsbuild-plugin-protobufjs",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "version": "changeset version",
    "postversion": "npm run build",
    "prerelease": "npm run build",
    "release": "changeset publish"
  },
  "dependencies": {
    "protobufjs-cli": "^1.1.3"
  },
  "peerDependencies": {
    "protobufjs": "*"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@changesets/cli": "^2.29.6",
    "@rsbuild/core": "^1.5.3",
    "@rslib/core": "^0.12.4",
    "@types/node": "^22.18.0",
    "protobufjs": "^7.5.4",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baranwang/rsbuild-plugin-protobufjs.git"
  },
  "license": "MIT",
  "keywords": [
    "rsbuild",
    "protobufjs"
  ]
}
